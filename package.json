{
  "name": "portal-class",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint:fix": "yarn eslint:fix && yarn stylelint:fix && yarn prettier:fix",
    "check-types": "tsc --noemit",
    "eslint": "eslint --cache --ext .ts,.vue --ignore-path .gitignore .",
    "eslint:fix": "yarn eslint --fix",
    "stylelint": "stylelint --cache src/**/*.{css,scss,vue}",
    "stylelint:fix": "yarn stylelint --fix",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check '**/*.ts' '**/*.vue'",
    "prettier:fix": "prettier --no-error-on-unmatched-pattern --write '**/*.ts' '**/*.vue'"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^14.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
